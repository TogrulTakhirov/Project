@using Otomation.Models.Sınıflar
@model List<SatisHaraket>
<div class="card-body">
    <table id="example1" class="table table-bordered table-striped">
        <thead>
            <tr>
                <th>ID</th>
                <th> Tarihi</th>
                <th> Adet</th>
                <th> Fiyat</th>
                <th> Toplam Tutar</th>
                <th> Ürün </th>
                <th> Cari </th>





            </tr>
        </thead>
        <tbody>
            @foreach (var k in Model)
            {
                <tr>
                    <td>
                        @k.Satisid
                    </td>
                    <td>
                        @k.Tarihi
                    </td>
                    <td>
                        @k.Adet
                    </td>
                    <td>
                        @k.Fiyat
                    </td>
                    <td>
                        @k.ToplamFutar
                    </td>
                    <td>
                        @k.Urun.UrunAd
                    </td>
                    <td>
                        @k.Cari.CariAd
                    </td>


                </tr>

            }

        </tbody>

    </table>
</div>