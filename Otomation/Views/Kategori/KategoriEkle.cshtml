@model Otomation.Models.Sınıflar.Kategori
@{
    ViewBag.Title = "KategoriEkle";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Kategori Ekle</h2>
<br />
 <form class="form-group" method="post">

     @Html.LabelFor(x=>x.KategoriAd)
     @Html.TextBoxFor(x => x.KategoriAd,new {@class="form-control"})
     <br />
     <button class="btn btn-outline-info" type="submit" id="submitButton"> Ekle</button>

 </form>